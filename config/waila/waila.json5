{
  general: {
    // Add to Vanilla's Options
    // Whether to add a button to vanilla game's options that opens WTHIT config screen
    // Default value: [true]
    vanillaOptions: true,
    // Отображать подсказки
    // Собирать данные и отображать подсказки
    // Default value: [true]
    displayTooltip: true,
    // Умные данные
    // Основная информация скрыта, пока вы не пригнетесь
    // Default value: [false]
    shiftForDetails: false,
    // Не упоминать о детялях при приседании
    // Скрыть текст "Присядьте для деталей" если соответствующая опция включена
    // Default value: [false]
    hideShiftText: false,
    // Режим отображения
    // Поведение всплывающей подсказки
    // Переключать: включать и выключать подсказку соответствующей клавишей
    // Удерживать: всплывающая подсказка будет отображаться до тех пор, пока зажата соответствующая клавиша
    // Default value: [TOGGLE]
    // Available values: [HOLD_KEY, TOGGLE]
    displayMode: "TOGGLE",
    // Скрыть при списке игроков
    // Скрывает подсказки при открытом списке игроков
    // Default value: [true]
    hideFromPlayerList: true,
    // Скрыть при отладке
    // Скрывает подсказки во время режима отладки
    // Default value: [true]
    hideFromDebug: true,
    // Включить TTS
    // Читает названия блоков и существ используя синтез речи
    // Default value: [false]
    enableTextToSpeech: false,
    // Лимит запросов
    // Сколько миллисекунд должно пройти до следующего запроса к серверу
    // Должно быть как минимум 250 миллисекунд
    // Default value: [250]
    rateLimit: 250
  },
  overlay: {
    position: {
      align: {
        // Default value: [CENTER]
        // Available values: [LEFT, CENTER, RIGHT]
        x: "CENTER",
        // Default value: [TOP]
        // Available values: [TOP, MIDDLE, BOTTOM]
        y: "TOP"
      },
      anchor: {
        // Default value: [CENTER]
        // Available values: [LEFT, CENTER, RIGHT]
        x: "CENTER",
        // Default value: [TOP]
        // Available values: [TOP, MIDDLE, BOTTOM]
        y: "TOP"
      },
      // Смещение
      // Default value: [0]
      x: 0,
      // Default value: [0]
      y: 0,
      // Перекрытие босс-бара
      // Если Нет, то всплывающая подсказка будет двигаться вниз при любом активном босс-баре.
      // Влияет только на выравнивание по центру вверху.
      // Default value: [false]
      bossBarsOverlap: false
    },
    color: {
      // Прозрачность
      // Default value: [204]
      backgroundAlpha: 204,
      // Custom Themes
      themes: [],
      // Тема
      // Default value: [waila:vanilla]
      activeTheme: "waila:vanilla"
    },
    // Размер
    // Насколько большой будет подсказка
    // 1.0 это стандарное значение
    // Default value: [1.0]
    scale: 1.0,
    // Лимит FPS для подсказки
    // Снижает частоту отрисовки подсказки для общего увеличения FPS
    // Установите 0 чтобы снять лимит
    // Default value: [30]
    fps: 30
  },
  // Text formatters
  formatter: {
    // Default value: [§9§o%s]
    modName: "§9§o%s",
    // Default value: [§f%s]
    blockName: "§f%s",
    // Default value: [§f%s]
    fluidName: "§f%s",
    // Default value: [§f%s]
    entityName: "§f%s",
    // Default value: [§8%s]
    registryName: "§8%s",
    // 
    styles: {
      // 
      "§f%s": {
        // 
        color: {
          // 
          value: 16777215,
          // 
          name: "white"
        },
        // 
        bold: false,
        // 
        italic: false,
        // 
        underlined: false,
        // 
        strikethrough: false,
        // 
        obfuscated: false,
        // 
        clickEvent: null,
        // 
        hoverEvent: null,
        // 
        insertion: null,
        // 
        font: null
      }
    }
  },
  keyBinds: {
    // Открыть конфиг
    // Default value: [key.keyboard.unknown]
    openConfig: "key.keyboard.unknown",
    // Отображать оверлей
    // Default value: [key.keyboard.unknown]
    showOverlay: "key.keyboard.unknown",
    // Отображать жидкости
    // Default value: [key.keyboard.unknown]
    toggleLiquid: "key.keyboard.unknown",
    // Отображать применение в рецептах
    // Default value: [key.keyboard.unknown]
    showRecipeInput: "key.keyboard.unknown",
    // Отображать рецепты
    // Default value: [key.keyboard.unknown]
    showRecipeOutput: "key.keyboard.unknown"
  },
  // Internal value, DO NOT TOUCH!
  // Default value: [0]
  configVersion: 1
}